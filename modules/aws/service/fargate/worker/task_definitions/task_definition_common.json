[
    {
      "name": "${project}-${environment}-${service_name}-worker-container",
      "image": "${worker_repository_url}:latest",
      "readonlyRootFilesystem": false,
      "essential": true,
      "environmentFiles": [],
      "networkMode": "awsvpc",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/${project}-${environment}-${service_name}",
          "awslogs-region": "${region}",
          "awslogs-stream-prefix": "worker"
        }
      }
    }
  ]